<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.anadadake.crud.common.entity.UserRoleEntity" table="t_user_role">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="userNo">
            <column name="USER_NO" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="roleCode">
            <column name="ROLE_CODE" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>