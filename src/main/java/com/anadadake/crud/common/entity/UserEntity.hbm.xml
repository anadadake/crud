<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.anadadake.crud.common.entity.UserEntity" table="t_user">
<!--        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>-->
        <property name="userNo">
            <column name="USER_NO" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="text"/>
        </property>
        <property name="mobilePhone">
            <column name="MOBILE_PHONE" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="email">
            <column name="EMAIL" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="createUser">
            <column name="CREATE_USER" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="createTime">
            <column name="CREATE_TIME" sql-type="datetime"/>
        </property>
        <property name="updateUser">
            <column name="UPDATE_USER" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="updateTime">
            <column name="UPDATE_TIME" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>