<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.anadadake.crud.common.entity.RoleFuncEntity" table="t_role_func">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="roleCode">
            <column name="ROLE_CODE" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="funcCode">
            <column name="FUNC_CODE" sql-type="varchar(15)" length="15"/>
        </property>
    </class>
</hibernate-mapping>